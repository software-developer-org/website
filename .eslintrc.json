{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "jest": true
    },
    "extends": ["plugin:jest-playwright/recommended", "prettier"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "indent": ["error", 2],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "eol-last": ["error", "always"]
    }
  }